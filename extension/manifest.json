{
  "manifest_version": 3,
  "name": "Upwork Bid Checker",
  "version": "1.1",
  "description": "Save Upwork Bid URL to the team Google Sheet. by Yajuvendra.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://script.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service_worker.js"
  }
}